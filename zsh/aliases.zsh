# zsh/aliases.zsh
# General aliases
alias ll='ls -la'
alias h='history'
alias c='clear'

# Git aliases
alias gs='git status'
alias gp='git pull'
alias gps='git push'
alias gc='git commit -m'

# AWS aliases
alias awsw='aws who-am-i'
alias awsp='aws configure list-profiles'

# Terraform aliases
alias tf='terraform'
alias tfi='terraform init'
alias tfp='terraform plan'
alias tfa='terraform apply'